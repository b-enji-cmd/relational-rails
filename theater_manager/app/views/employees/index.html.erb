<h1>All Employees</h1>

<% @employees.each do |employee| %>
	<h3><a href="/employees/<%= employee.id%>"><%= employee.name%></a></h3>
	<p>Full time?: <%= employee.is_full_time%></p>
	<p>Hours Worked: <%= employee.hours_worked%></p>
<% end %>